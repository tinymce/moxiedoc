= {{title}}
:description: "{{desc}}"
:keywords: "{{keywords}}"
:title_nav: "{{title}}"
:moxie-type: "folder"

{% assign navigation = site.data.nav_api %}
{% for entry in navigation %}
  {% if entry.url == "api" %}
    {% for subentry in entry.pages %}
      {% if subentry.url == "{{title}}" %}
        {% assign links = subentry.pages %}
      {% endif %}
    {% endfor %}
  {% endif %}
{% endfor %}
{% include index.html links=links sectionTitle="{{title}}" %}
