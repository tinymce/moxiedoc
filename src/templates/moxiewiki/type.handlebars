<div class="api classdetails">
  {{#if subMixinOrSuperTypes}}
    <div class="classinfo">
      <table>
        <tbody>
          {{#if superTypes}}
          <tr>
            <td class="first">Inheritance</td>
            <td class="last">
              <ul class="inherits">
                <li class="first">{{name}}</li>
                {{#each superTypes}}
                <li><a href="{{link}}">{{name}}</a></li>
                {{/each}}
              </ul>
            </td>
          </tr>
          {{/if}}

          {{#if mixesTypes}}
          <tr>
            <td class="first">Mixes</td>
            <td class="last">
              {{#each mixesTypes}}
              <a href="{{link}}">{{name}}</a>
              {{/each}}
            </td>
          </tr>
          {{/if}}

          {{#if mixinTypes}}
          <tr>
            <td class="first">Mixins</td>
            <td class="last">
              {{#each mixinTypes}}
              <a href="{{link}}">{{name}}</a>
              {{/each}}
            </td>
          </tr>
          {{/if}}

          {{#if subTypes}}
          <tr>
            <td class="first">Subclasses</td>
            <td class="last">
              {{#each subTypes}}
              <a href="{{link}}">{{name}}</a>
              {{/each}}
            </td>
          </tr>
          {{/if}}
        </tbody>
      </table>
    </div>
  {{/if}}

  <div class="description">{{desc}}</div>

  {{#if examples}}
    <div class="examples">
    <h2>Examples</h2>
{{#each examples}}
<pre class="js">{{content}}</pre>
{{/each}}
    </div>
  {{/if}}

{{#if settings}}
  <div class="settings">
    <h2>Settings</h2>
    <table>
      <thead class="small">
        <tr>
          <th class="first">Setting</th>
          <th class="last">Defined By</th>
        </tr>
      </thead>
      <tbody>
        {{#each settings}}
        <tr>
          <td class="first">
            {{#if isStatic}}
              <div><a href="{{link}}" class="member">{{name}}</a> <span class="static">static</span></div>
            {{else}}
              <div><a href="{{link}}" class="member">{{name}}</a></div>
            {{/if}}
            <div>{{summary}}</div>
          </td>
          <td class="last">
            {{#if definedinLink}}
              <a href="{{definedinLink}}">{{definedin}}</a>
            {{else}}
              {{definedin}}
            {{/if}}
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
{{/if}}

